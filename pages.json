	"pages": [{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "index",
				"app-plus": {
					"titleNView": false
				}
			}
		},
		{
			"path": "pages/login/login",
			"param": {
				"test": "1111"
			},
			"style": {
