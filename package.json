    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix --quiet",
    "czc": "git add . && cz",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "up:uni": "npx @dcloudio/uvm alpha",
